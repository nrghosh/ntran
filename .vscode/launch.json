    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug analyzer",
            "type": "debugpy",
            "request": "launch",
            "program": "ntran/analyze.py",
            "console": "integratedTerminal",
            "args": [
                "./ntran/results"
            ]
        },
        {
            "name": "Debug cold-neondb",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "ntran/main.go",
            "args": [
                "-policy",
                "cold-neondb",
            ]
        },
        {
            "name": "Debug prewarm-neondb",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "ntran/main.go",
            "args": [
                "-policy",
                "prewarm-neondb",
            ]
        },
        {
            "name": "Debug duckdb",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "ntran/main.go",
            "args": [
                "-policy",
                "duckdb",
            ]
        }
    ]
